            <property name = "showFinger" value = "limbs.Finger"/>
            <property name = "midFinger" value = "limbs.Finger"/>
            <property name = "littleFinger" value = "limbs.Finger"/>
            <property name = "unnamedFinger" value = "limbs.Finger"/>
        </bodyPart>
        <bodyPart name = "cubit" class = "limbs.Cubit">
            <property name = "mobile" value = "true"/>
        </bodyPart>
        <property name = "cubit" value = "limbs.Cubit"/>
        <property name = "hand" value = "limbs.Hand"/>
        <property name = "operable" value = "true"/>
    </bodyPart>
    <bodyPart name = "body" class = "Body">
        <property name = "constitution" value = "ATHLETIC"/>
        <property name = "tattoo" value = "ЛЕНИН! ПАРТИЯ! КОМСОМОЛ!"/>
