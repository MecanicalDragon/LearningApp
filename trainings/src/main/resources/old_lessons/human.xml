        </bodyPart>
        <bodyPart name = "nose" class = "head.Nose">
            <property name = "size" value = "MID"/>
        </bodyPart>
        <bodyPart name = "mouth" class = "head.Mouth">
            <property name = "speech" value = "Оставайся, мальчик с нами - будешь нашим королём!"/>
