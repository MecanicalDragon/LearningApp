        </bodyPart>
        <bodyPart name = "ear" class = "head.Ear">
            <property name = "lopEared" value = "false"/>
            <property name = "hearing" value = "5"/>
        </bodyPart>
        <property name = "leftEye" value = "head.Eye"/>
